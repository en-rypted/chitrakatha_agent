{
    "name": "chitrakatha_agent",
    "version": "1.0.0",
    "description": "Local agent for P2P file transfer",
    "main": "index.js",
    "type": "module",
    "bin": "index.js",
    "pkg": {
        "targets": [
            "node18-win-x64"
        ],
        "outputPath": "dist"
    },
    "scripts": {
        "start": "node index.js",
        "build": "pkg . --output dist/chitrakatha_agent"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "ffmpeg-static": "^5.3.0",
        "ffprobe-static": "^3.1.0",
        "fluent-ffmpeg": "^2.1.3",
        "ip": "^1.1.8",
        "magnet-uri": "^7.0.7",
        "range-parser": "^1.2.1",
        "socket.io-client": "^4.7.4",
        "webtorrent": "^2.8.5"
    }
}
